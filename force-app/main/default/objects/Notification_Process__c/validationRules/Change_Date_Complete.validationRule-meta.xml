<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Change_Date_Complete</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
	 ISPICKVAL(Type__c, &apos;Simple&apos;),
  ISBLANK(Change_date__c)
				)</errorConditionFormula>
    <errorDisplayField>Change_date__c</errorDisplayField>
    <errorMessage>You must complete the Change Date field.</errorMessage>
</ValidationRule>
