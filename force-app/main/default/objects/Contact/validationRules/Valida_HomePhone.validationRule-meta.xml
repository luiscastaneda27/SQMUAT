<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valida_HomePhone</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK(HomePhone)) &amp;&amp;
LEFT(HomePhone, 1) &lt;&gt; &quot;+&quot;</errorConditionFormula>
    <errorDisplayField>HomePhone</errorDisplayField>
    <errorMessage>The phone number must start with the &quot;+&quot; character</errorMessage>
</ValidationRule>
